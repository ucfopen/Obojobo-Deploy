{
  "name": "obojobo-deploy",
  "version": "1.0.0",
  "description": "Reference project for deploying and customizing an Obojobo Next server",
  "main": "index.js",
  "private": true,
  "license": "AGPL-3.0-only",
  "scripts": {
    "start": "pm2 startOrGracefulReload pm2.yaml --update-env",
    "status": "pm2 status obojobo",
    "stop": "pm2 stop obojobo",
    "delete": "pm2 delete obojobo",
    "logs": "pm2 logs",
    "obojobo-migrate": "yarn run withenv ./node_modules/.bin/obojobo-migrate",
    "withenv": "node -r ./preloadEnv.js"
  },
  "dependencies": {
    "aws-secrets-environment": "^1.0.0",
    "deasync": "^0.1.15",
    "dotenv": "^8.0.0",
    "obojobo-chunks-action-button": "7.0.1",
    "obojobo-chunks-break": "7.0.1",
    "obojobo-chunks-code": "7.0.1",
    "obojobo-chunks-figure": "7.0.1",
    "obojobo-chunks-heading": "7.0.1",
    "obojobo-chunks-html": "7.0.1",
    "obojobo-chunks-iframe": "7.0.1",
    "obojobo-chunks-list": "7.0.1",
    "obojobo-chunks-math-equation": "7.0.1",
    "obojobo-chunks-multiple-choice-assessment": "7.0.1",
    "obojobo-chunks-question": "7.0.1",
    "obojobo-chunks-question-bank": "7.0.1",
    "obojobo-chunks-table": "7.0.1",
    "obojobo-chunks-text": "7.0.1",
    "obojobo-chunks-youtube": "7.0.1",
    "obojobo-document-engine": "7.0.1",
    "obojobo-document-json-parser": "7.0.1",
    "obojobo-document-xml-parser": "7.0.1",
    "obojobo-express": "7.0.1",
    "obojobo-lib-utils": "7.0.1",
    "obojobo-module-selector": "7.0.1",
    "obojobo-modules-module": "7.0.1",
    "obojobo-pages-page": "7.0.1",
    "obojobo-repository": "7.0.1",
    "obojobo-sections-assessment": "7.0.1",
    "obojobo-sections-content": "7.0.1",
    "pm2": "^3.5.1"
  }
}
