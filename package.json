{
  "name": "obojobo-deploy",
  "version": "1.0.0",
  "description": "Reference project for deploying and customizing an Obojobo Next server",
  "main": "index.js",
  "private": true,
  "license": "AGPL-3.0-only",
  "scripts": {
    "start": "pm2 startOrGracefulReload pm2.yaml --update-env",
    "status": "pm2 status obojobo",
    "stop": "pm2 stop obojobo",
    "delete": "pm2 delete obojobo",
    "logs": "pm2 logs",
    "db-migrate": "yarn run withenv node_modules/.bin/db-migrate --config ./node_modules/obojobo-express/config/db.json --migrations-dir ./node_modules/obojobo-express/migrations/",
    "withenv": "node -r ./node_modules/dotenv/config"
  },
  "dependencies": {
    "aws-secrets-environment": "^1.0.0",
    "dotenv": "^8.0.0",
    "obojobo-document-engine" : "4.1.2",
    "obojobo-document-xml-parser" : "4.1.2",
    "obojobo-express" : "4.1.2",
    "obojobo-chunks-action-button" : "4.1.2",
    "obojobo-chunks-break" : "4.1.2",
    "obojobo-chunks-code" : "4.1.2",
    "obojobo-chunks-figure" : "4.1.2",
    "obojobo-chunks-heading" : "4.1.2",
    "obojobo-chunks-html" : "4.1.2",
    "obojobo-chunks-iframe" : "4.1.2",
    "obojobo-chunks-list" : "4.1.2",
    "obojobo-chunks-math-equation" : "4.1.2",
    "obojobo-chunks-multiple-choice-assessment" : "4.1.2",
    "obojobo-chunks-question" : "4.1.2",
    "obojobo-chunks-question-bank" : "4.1.2",
    "obojobo-chunks-table" : "4.1.2",
    "obojobo-chunks-text" : "4.1.2",
    "obojobo-chunks-youtube" : "4.1.2",
    "obojobo-modules-module" : "4.1.2",
    "obojobo-pages-page" : "4.1.2",
    "obojobo-sections-assessment" : "4.1.2",
    "obojobo-sections-content" : "4.1.2",
    "pm2": "^3.5.1"
  }
}
