{
  "name": "obojobo-deploy",
  "version": "1.0.0",
  "description": "Reference project for deploying and customizing an Obojobo Next server",
  "main": "index.js",
  "private": true,
  "license": "AGPL-3.0-only",
  "scripts": {
    "start": "pm2 startOrGracefulReload pm2.yaml --update-env",
    "status": "pm2 status obojobo",
    "stop": "pm2 stop obojobo",
    "delete": "pm2 delete obojobo",
    "logs": "pm2 logs",
    "db-migrate": "yarn run withenv node_modules/.bin/db-migrate --config ./node_modules/obojobo-express/config/db.json --migrations-dir ./node_modules/obojobo-express/migrations/",
    "withenv": "node -r ./node_modules/dotenv/config"
  },
  "dependencies": {
    "aws-secrets-environment": "^1.0.0",
    "dotenv": "^8.0.0",
    "obojobo-document-engine": "5.0.0-alpha.0",
    "obojobo-document-xml-parser": "5.0.0-alpha.0",
    "obojobo-express": "5.0.0-alpha.0",
    "obojobo-chunks-action-button": "5.0.0-alpha.0",
    "obojobo-chunks-break": "5.0.0-alpha.0",
    "obojobo-chunks-code": "5.0.0-alpha.0",
    "obojobo-chunks-figure": "5.0.0-alpha.0",
    "obojobo-chunks-heading": "5.0.0-alpha.0",
    "obojobo-chunks-html": "5.0.0-alpha.0",
    "obojobo-chunks-iframe": "5.0.0-alpha.0",
    "obojobo-chunks-list": "5.0.0-alpha.0",
    "obojobo-chunks-math-equation": "5.0.0-alpha.0",
    "obojobo-chunks-multiple-choice-assessment": "5.0.0-alpha.0",
    "obojobo-chunks-question": "5.0.0-alpha.0",
    "obojobo-chunks-question-bank": "5.0.0-alpha.0",
    "obojobo-chunks-table": "5.0.0-alpha.0",
    "obojobo-chunks-text": "5.0.0-alpha.0",
    "obojobo-chunks-youtube": "5.0.0-alpha.0",
    "obojobo-modules-module": "5.0.0-alpha.0",
    "obojobo-pages-page": "5.0.0-alpha.0",
    "obojobo-sections-assessment": "5.0.0-alpha.0",
    "obojobo-sections-content": "5.0.0-alpha.0",
    "obojobo-lib-utils": "5.0.0-alpha.0",
    "pm2": "^3.5.1"
  }
}
